# LuLu Ventures Environment Variables Example
# Copy this file to .env and update the values as needed

# Application Settings
VITE_APP_TITLE=LuLu Ventures
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3000

# External Links
VITE_YOUTUBE_CHANNEL_URL=https://www.youtube.com/@TheDeepValue
VITE_COMPANY_EMAIL=info@luluventures.com

# Development Settings
VITE_DEV_MODE=true

# AWS Cognito Configuration
VITE_AWS_USER_POOL_ID=your_user_pool_id
VITE_AWS_USER_POOL_CLIENT_ID=your_client_id
VITE_AWS_IDENTITY_POOL_ID=your_identity_pool_id
VITE_AWS_COGNITO_DOMAIN=your_cognito_domain.auth.region.amazoncognito.com
VITE_REDIRECT_SIGN_IN=http://localhost:5173/
VITE_REDIRECT_SIGN_OUT=http://localhost:5173/

# AWS DynamoDB Configuration
VITE_AWS_REGION=us-west-2
VITE_AWS_ACCESS_KEY_ID=your_access_key_id
VITE_AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Production Environment Example (uncomment and update for production)
# VITE_API_BASE_URL=https://api.luluventures.com
# VITE_DEV_MODE=false